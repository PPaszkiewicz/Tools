<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StableTextView">
        <attr name="stableSizePolicy" format="enum">
            <enum name="length_changed" value="1" />
            <enum name="always" value="0" />
            <enum name="never" value="2" />
            <enum name="only_once" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="StableChronometer">
        <attr name="stableSizePolicy" format="enum">
            <enum name="length_changed" value="1" />
            <enum name="always" value="0" />
            <enum name="never" value="2" />
            <enum name="only_once" value="3" />
        </attr>
    </declare-styleable>
</resources>